	<dataset id="HF213">
		<alternateIdentifier>HF213</alternateIdentifier>
		<title>Forest Inventory for Tree Demography and Carbohydrate Reserves at Harvard Forest 2009-2011</title>
		<creator>
			<individualName>
				<givenName>Michael</givenName>
				<surName>Dietze</surName>
			</individualName>
		</creator>
		<creator>
