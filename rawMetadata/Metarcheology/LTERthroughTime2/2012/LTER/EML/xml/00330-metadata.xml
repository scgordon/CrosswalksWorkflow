         <attributeList>
            <attribute id="1330529460526">
               <attributeName>site</attributeName>
               <attributeDefinition>site identifier</attributeDefinition>
               <measurementScale>
                  <nominal>
                     <nonNumericDomain>
                        <textDomain>
                           <definition>site identifier</definition>
                        </textDomain>
                     </nonNumericDomain>
                  </nominal>
               </measurementScale>
            </attribute>
            <attribute id="1330529460541">
               <attributeName>lu_class</attributeName>
               <attributeDefinition>land use class (forest, residential, other developed) from 2005 MassGIS landuse layer.  http://www.mass.gov/mgis/lus2005.htm</attributeDefinition>
               <measurementScale>
                  <nominal>
                     <nonNumericDomain>
                        <textDomain>
                           <definition>land use class</definition>
                        </textDomain>
                     </nonNumericDomain>
                  </nominal>
               </measurementScale>
            </attribute>
            <attribute id="1330529460557">
               <attributeName>date</attributeName>
               <attributeDefinition>date that the plot was visited to survey biomass, collect soil and leaf samples, and assess other physical characteristics.</attributeDefinition>
               <measurementScale>
                  <dateTime>
                     <formatString>YYYY-MM-DD</formatString>
                  </dateTime>
               </measurementScale>
            </attribute>
            <attribute id="1330529460572">
               <attributeName>live_tree_c</attributeName>
               <attributeDefinition>live tree carbon in Mg C/ha.  Based on DBH (diameter at breast height) measurements of trees 5cm or larger and allometric equations.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <customUnit>megagramPerHectare</customUnit>
                     </unit>
                     <precision>0.01</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </ratio>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460588">
               <attributeName>total_cwd_c</attributeName>
               <attributeDefinition>total coarse woody debris (CWD) in Mg C/ha. A value of 0 indicated that no CWD was present at the cite. The minimum size requirements were 10cm in diameter and 1m in length.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <customUnit>megagramPerHectare</customUnit>
                     </unit>
                     <precision>0.01</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </ratio>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460604">
               <attributeName>bd</attributeName>
               <attributeDefinition>bulk density of 0-10 cm mineral soil core (mean of 2-3 soil cores collected at the site). An NA value indicates that we were unable to collect a sample.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <standardUnit>gramsPerCubicCentimeter</standardUnit>
                     </unit>
                     <precision>0.001</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </ratio>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460619">
               <attributeName>n_perc</attributeName>
               <attributeDefinition>nitrogen percentage of 0 - 10 cm mineral soil (mean of 2-3 soil cores collected at the site). An NA value indicates that we were unable to collect a sample.</attributeDefinition>
               <measurementScale>
                  <interval>
                     <unit>
                        <standardUnit>dimensionless</standardUnit>
                     </unit>
                     <precision>0.001</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </interval>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460635">
               <attributeName>c_perc</attributeName>
               <attributeDefinition>carbon percentage of 0 - 10 cm mineral soil (mean of soil cores collected at the site). An NA value indicates that we were unable to collect a sample.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <standardUnit>dimensionless</standardUnit>
                     </unit>
                     <precision>0.001</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </ratio>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460651">
               <attributeName>c_to_n</attributeName>
               <attributeDefinition>C:N of 0 - 10 cm soil cores (mean of 2-3 soil cores collected at the site). An NA value indicates that we were unable to collect a sample.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <standardUnit>dimensionless</standardUnit>
                     </unit>
                     <precision>0.001</precision>
                     <numericDomain>
                        <numberType>real</numberType>
                     </numericDomain>
                  </ratio>
               </measurementScale>
               <missingValueCode>
                  <code>NA</code>
                  <codeExplanation>missing value</codeExplanation>
               </missingValueCode>
            </attribute>
            <attribute id="1330529460666">
               <attributeName>n_g_m2_10cm</attributeName>
               <attributeDefinition>measured nitrogen content of 0 - 10 cm mineral soil in g N/m2. An NA value indicates that we were unable to collect a sample.</attributeDefinition>
               <measurementScale>
                  <ratio>
                     <unit>
                        <standardUnit>gramsPerSquareMeter</standardUnit>
