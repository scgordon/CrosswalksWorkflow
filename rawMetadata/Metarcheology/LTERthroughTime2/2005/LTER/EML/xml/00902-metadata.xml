<section> 
 <title>Related Documents</title>
  <para> This is the SAS job used to tabulate the areas: </para>
  <para><literalLayout>
options linesize=80; 


Data wslandc; 


/* Change the name of the file in quotes - match the file name you want 
as 
    input to this program */
  length landc $ 30;
  INFILE 'wsccap83.dat' MISSOVER dlm=',' dsd missover; 


Input  AREA PERIM junk1 WSCCAPID junk2 WSHEDID junk3 landId GRIDCODE ; 



/* get rid of watershed 1 since it is fishermans island */
 if (wshedid GT 1); 


area=area/10000; 


Label wshedid='Watershed Number';
  label area='Area (ha)'; 


if (gridcode eq 1) then landc='Developed-High Intensity';
if (gridcode eq 2) then landc='Developed-Low Intensity';
if (gridcode eq 3) then landc='Cropland';
if (gridcode eq 4) then landc='Grassland';
if (gridcode eq 5) then landc='Deciduous Forest';
if (gridcode eq 6) then landc='Evergreen Forest';
if (gridcode eq 7) then landc='Mixed Forest';
if (gridcode eq 8) then landc='Mixed Shrub/Scrub';
if (gridcode eq 9) then landc='Palustrine Forest';
if (gridcode eq 10) then landc='Estuarine Emergent Wetland';
if (gridcode eq 11) then landc='Palustrine Emergent Wetland';
if (gridcode eq 12) then landc='Tidal Flats';
if (gridcode eq 13) then landc='Exposed Land (Bare Soil+Sand)';
if (gridcode eq 14) then landc='Water'; 


proc freq;
   tables landc;
   weight area; 


tables wshedid*landc / out=landc;
   weight area;
run; 


proc freq data=wslandc;
   tables wshedid*gridcode /out=gridc;
   weight area;
run; 


proc sort data=gridc; by wshedid gridcode; 


data g1;
   set gridc;
   if (gridcode eq 1);
   rename count=develhi;
   drop percent gridcode; 


data g2;
   set gridc;
   if (gridcode eq 2);
   rename count=devello;
data g3;
   set gridc;
   if (gridcode eq 3);
   rename count=cropland;
   drop percent gridcode; 


data g4;
   set gridc;
   if (gridcode eq 4);
   rename count=grasslnd;
   drop percent gridcode; 


data g5;
   set gridc;
   if (gridcode eq 5);
   rename count=DecForst;
   drop percent gridcode; 


data g6;
   set gridc;
   if (gridcode eq 6);
   rename count=EvrForst;   drop percent gridcode; 


data g7;
   set gridc;
   if (gridcode eq 7);
   rename count=MxdForst;
   drop percent gridcode; 


data g8;
   set gridc;
   if (gridcode eq 8);
   rename count=MxdShrub;
   drop percent gridcode; 


data g9;
   set gridc;
   if (gridcode eq 9);
   rename count=PalForst;
   drop percent gridcode; 


data g10;
   set gridc;   rename count=EstWetld;
   drop percent gridcode; 


data g11;
   set gridc;
   if (gridcode eq 11);
   rename count=PalWetld;
   drop percent gridcode; 


data g12;
   set gridc;
   if (gridcode eq 12);
   rename count=TidalFlt;
   drop percent gridcode; 


data g13;
   set gridc;
   if (gridcode eq 13);
   rename count=Bare;
   drop percent gridcode; 


data g14;
   set gridc;   if (gridcode eq 14);
   rename count=water;
   drop percent gridcode; 


data all;
   merge g1 g2 g3 g4 g5 g6 g7 g8 g9 g10 g11 g12 g13 g14;
   by wshedid;
   array landc develhi--water;
   do i=1 to 14;
     if (landc[i]=.) then landc[i]=0;
   end;
   drop i; 


file 'landc.dat';
   put wshedid develhi--water; 


proc print; 


run; 


if (gridcode eq 10); 


drop percent gridcode; 


proc freq; 

</literalLayout></para>
