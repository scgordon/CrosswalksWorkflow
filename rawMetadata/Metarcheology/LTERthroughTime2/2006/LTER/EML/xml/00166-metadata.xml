   <dataset id="HF059">
      <alternateIdentifier>HF059</alternateIdentifier>
      <title>Canopy Photosynthesis Study at Harvard Forest 1991-1992</title>
      <creator>
         <individualName>
            <givenName>Susan</givenName>
            <surName>Bassow</surName>
