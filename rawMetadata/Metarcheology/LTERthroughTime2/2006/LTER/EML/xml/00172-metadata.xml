   <dataset id="HF011">
      <alternateIdentifier>HF011</alternateIdentifier>
      <title>Landscape and Regional Impacts of Hurricanes in New England 1620-1997</title>
      <creator>
         <individualName>
            <givenName>Emery</givenName>
            <surName>Boose</surName>
         </individualName>
      </creator>
