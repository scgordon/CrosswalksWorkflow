   <dataset id="HF208">
      <alternateIdentifier>HF208</alternateIdentifier>
      <title>Mark-Recapture of Rodent and Shrew Populations in a Declining Hemlock Stand at Harvard Forest 2012</title>
      <creator>
         <individualName>
            <givenName>Allyson</givenName>
