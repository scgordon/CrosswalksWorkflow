<methods>
      <methodStep>
        <description>
          <para>
            <literalLayout> 
             The dissolved gas and tritium samples were analyzed at the Rare Gas Lab at the University of Rochester, Rochester, NY
            </literalLayout>
          </para>
        </description>
      </methodStep>
        <methodStep>
            <description>
                <para>
                    <literalLayout> 
                     The stable isotope samples were analyzed at INSTAAR, University of Colorado at Boulder .
                    </literalLayout>
                </para>
            </description>
        </methodStep>
        <methodStep>
            <description>
                <para>
                    <literalLayout> 
                     The major ions were analyzed at the MCM-LTER Laboratory at Crary Laboratory in McMurdo Station, Antarctica.
                    </literalLayout>
                </para>
            </description>
        </methodStep>
              </methods>
