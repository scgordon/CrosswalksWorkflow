   <dataset id="HF179">
      <alternateIdentifier>HF179</alternateIdentifier>
      <title>Inorganic Nitrogen Pools and Tree Composition in Hemlock Removal Experiment at Harvard Forest 2007-2009</title>
      <creator>
         <individualName>
            <givenName>Tara</givenName>
