   <dataset id="HF177">
      <alternateIdentifier>HF177</alternateIdentifier>
      <title>Soil Arthropods in Hemlock Removal Experiment at Harvard Forest 2008</title>
      <creator>
         <individualName>
            <givenName>Tara</givenName>
            <surName>Sackett</surName>
         </individualName>
      </creator>
      <creator>
