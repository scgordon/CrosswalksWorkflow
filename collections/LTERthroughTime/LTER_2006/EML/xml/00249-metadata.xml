   <dataset id="HF046">
      <alternateIdentifier>HF046</alternateIdentifier>
      <title>Timber Harvesting Study in North Quabbin Region of Central Massachusetts 1984-2001</title>
      <creator>
         <individualName>
            <givenName>David</givenName>
            <surName>Kittredge</surName>
         </individualName>
      </creator>
      <creator>
         <individualName>
