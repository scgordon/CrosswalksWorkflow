   <dataset id="HF109">
      <alternateIdentifier>HF109</alternateIdentifier>
      <title>Effects of Prey Availability on Sarracenia Physiology at Harvard Forest 2005</title>
      <creator>
         <individualName>
            <givenName>Aaron</givenName>
            <surName>Ellison</surName>
         </individualName>
      </creator>
      <creator>
