      <attributeList>
        <attribute id="VCR15247.1.LEAF_ID">
          <attributeName>LEAF_ID</attributeName>
          <attributeLabel>Site and Leaf Identification Code</attributeLabel>
          <attributeDefinition>LEAF_ID - Site and Leaf Identification Code</attributeDefinition>
          <storageType typeSystem="http://www.w3.org/2001/XMLSchema-datatypes">string</storageType>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain enforced="yes">
                  <codeDefinition>
                    <code>BI</code>
                    <definition>Bayside site - inside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>BO</code>
                    <definition>Bayside site - outside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>CI</code>
                    <definition>Colonizing site - inside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>CO</code>
                    <definition>Colonizing site - outside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>MI</code>
                    <definition>Mid-Island site - inside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>MO</code>
                    <definition>Mid-Island site - outside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>YI</code>
                    <definition>Young site - inside thicket</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>YO</code>
                    <definition>Young site - outside thicket</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
          <methods>
            <methodStep>
              <description>
                <para> Codes consist of two letters, plus a number. The first letter is always the site identifier. The second letter indicates whether the sample comes from inside (I) our outside (O) the thicket. The number indicates the specific measurement number within each site. </para>
              </description>
            </methodStep>
          </methods>
        </attribute>
        <attribute id="VCR15247.1.LEAF_MASS">
          <attributeName>LEAF_MASS</attributeName>
          <attributeLabel>Mass of leaf</attributeLabel>
          <attributeDefinition>LEAF_MASS - Mass of leaf</attributeDefinition>
          <storageType typeSystem="http://www.w3.org/2001/XMLSchema-datatypes">float</storageType>
          <measurementScale>
            <interval>
              <unit>
                <customUnit>grams</customUnit>
              </unit>
              <numericDomain>
                <numberType>real</numberType>
                <bounds>
                  <minimum exclusive="false">0</minimum>
                </bounds>
              </numericDomain>
            </interval>
          </measurementScale>
          <methods>
            <methodStep>
              <description>
                <para> Leaves were dried at 70 degrees Celsus for 72 hours prior to weighing. </para>
              </description>
            </methodStep>
          </methods>
        </attribute>
        <attribute id="VCR15247.1.LEAF_AREA">
          <attributeName>LEAF_AREA</attributeName>
          <attributeLabel>Area of Leaf</attributeLabel>
          <attributeDefinition>LEAF_AREA - Area of Leaf</attributeDefinition>
          <storageType typeSystem="http://www.w3.org/2001/XMLSchema-datatypes">float</storageType>
          <measurementScale>
            <interval>
              <unit>
                <customUnit>squareCentimeters</customUnit>
              </unit>
              <numericDomain>
                <numberType>real</numberType>
                <bounds>
                  <minimum exclusive="false">0</minimum>
                </bounds>
              </numericDomain>
            </interval>
          </measurementScale>
          <methods>
            <methodStep>
              <description>
                <para> Leaf area was quantified using a leaf area meter (LI-3100C). </para>
              </description>
            </methodStep>
          </methods>
        </attribute>
        <attribute id="VCR15247.1.SLA_CM">
          <attributeName>SLA_CM</attributeName>
          <attributeLabel>Specific Leaf Area Square Centimeters per Gram</attributeLabel>
          <attributeDefinition>SLA_CM - Specific Leaf Area Square Centimeters per Gram</attributeDefinition>
