              </unit>
              <numericDomain>
                <numberType>real</numberType>
                <bounds>
                  <minimum exclusive="false">0</minimum>
                </bounds>
              </numericDomain>
            </interval>
          </measurementScale>
        </attribute>
        <attribute id="VCR15247.1.SLA_M">
