              <nonNumericDomain>
                <textDomain>
                  <definition>The unique numerical identification code assigned to each individual upon first capture.</definition>
                </textDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>Capture Type</attributeName>
          <attributeLabel>Capture Type</attributeLabel>
          <attributeDefinition>Defines whether an animal was newly captured and marked (i.e., new) or was already marked upon capture (i.e., recapture).</attributeDefinition>
          <storageType>string</storageType>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>N</code>
                    <definition>new</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>RC</code>
                    <definition>recapture</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>Sex</attributeName>
          <attributeLabel>Sex</attributeLabel>
          <attributeDefinition>Gender of a captured animal as determined from observation of genitals.</attributeDefinition>
          <storageType>string</storageType>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>M</code>
                    <definition>male</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>F</code>
                    <definition>female</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
        <attribute>
          <attributeName>Repro</attributeName>
          <attributeLabel>Repro</attributeLabel>
          <attributeDefinition>The reproductive condition of an animal based on observation of genitals and teats. This was typically only observed upon first capture each month.</attributeDefinition>
          <storageType>string</storageType>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>E</code>
                    <definition>Estrus (i.e., female with copulatory plug or swollen vulva).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>NR</code>
                    <definition>Nonreproductive (i.e., female with no sign of current or past reproduction).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>NS</code>
                    <definition>Non-scrotal (i.e., male with no visible scrotum and testes in abdominal cavity).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>P</code>
                    <definition>Pregnant (i.e., female with embryos that could be felt with palpitation).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>PL</code>
                    <definition>Past-lactation (i.e., female with signs of past lactation including long, white teats).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>RL</code>
                    <definition>Recent lactation (i.e., female with signs of recent lactation including milk, hairloss, and long, red teats).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>SA</code>
                    <definition>Scrotum, test abdominal (i.e., male with a visible scrotum, but retracted testes).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>SC</code>
                    <definition>Scrotal (i.e., male with testes in the scrotum).</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>UN</code>
                    <definition>Unknown reproductive condition.</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
          <missingValueCode>
            <code>-888</code>
            <codeExplanation>reproduction condition not taken after first capture</codeExplanation>
          </missingValueCode>
        </attribute>
        <attribute>
          <attributeName>Animal Mass</attributeName>
          <attributeLabel>Animal Mass</attributeLabel>
          <attributeDefinition>The mass of an individual upon capture. Mass was typically taken only upon first capture each month.</attributeDefinition>
          <measurementScale>
            <ratio>
              <unit>
