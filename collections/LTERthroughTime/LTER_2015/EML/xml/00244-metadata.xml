  <dataset id="HF077">
    <alternateIdentifier>HF077</alternateIdentifier>
    <title>Long-term Vegetation Dynamics in Southwestern New Hampshire from 13000 BP to Present</title>
    <creator>
      <individualName>
        <givenName>Peter</givenName>
        <surName>Schoonmaker</surName>
      </individualName>
    </creator>
    <creator>
