  <dataset id="HF123">
    <alternateIdentifier>HF123</alternateIdentifier>
    <title>Canopy LiDAR Measurements in Hemlock Removal Experiment at Harvard Forest 2005</title>
    <creator>
      <individualName>
        <givenName>John</givenName>
        <surName>Weishampel</surName>
      </individualName>
    </creator>
    <creator>
