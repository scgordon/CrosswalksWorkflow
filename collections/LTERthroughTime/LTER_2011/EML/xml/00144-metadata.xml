   <dataset id="HF189">
      <alternateIdentifier>HF189</alternateIdentifier>
      <title>Lake Sediment Pollen from Wildwood Lake in Long Island NY from 4500 BP to Present</title>
      <creator>
         <individualName>
            <givenName>Wyatt</givenName>
            <surName>Oswald</surName>
