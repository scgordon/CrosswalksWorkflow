                            <city>Phoenix</city>
                            <administrativeArea>AZ</administrativeArea>
                            <postalCode>85003</postalCode>
                        </address>
                        <electronicMailAddress>webmail@mail.maricopa.gov</electronicMailAddress>
                    </creator>
                    <contact>
                        <organizationName>Maricopa Association of Governments</organizationName>
                        <address>
                            <deliveryPoint>302 North 1st Avenue , Suite 300</deliveryPoint>
